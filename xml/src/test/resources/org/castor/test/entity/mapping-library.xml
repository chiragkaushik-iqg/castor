<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapping PUBLIC "-//EXOLAB/Castor Mapping DTD Version 1.0//EN"
   "http://castor.org/mapping.dtd">
<mapping>
  <class name="org.castor.test.entity.Library" auto-complete="true">
    <map-to xml="Library" ns-uri="http://castor.org/library"/>
    <field name="authors">
      <bind-xml name="Authors" node="element" ns-uri="http://castor.org/library"/>
    </field>
    <field name="books">
      <bind-xml name="Books" node="element" ns-uri="http://castor.org/library"/>
    </field>
  </class>
  <class name="org.castor.test.entity.Authors" auto-complete="true">
    <map-to xml="Authors" ns-uri="http://castor.org/library"/>
    <field name="author" type="org.castor.test.entity.Author" collection="array">
      <bind-xml name="Author" node="element" ns-uri="http://castor.org/library"/>
    </field>
  </class>
  <class name="org.castor.test.entity.Author" auto-complete="true">
    <map-to xml="Author" ns-uri="http://castor.org/library"/>
    <identity>
      <field name="id"/>
    </identity>
    <field name="id">
      <bind-xml name="id" node="attribute"/>
    </field>
    <field name="idref">
      <bind-xml name="idref" node="attribute"/>
    </field>
    <field name="name">
      <bind-xml name="Name" node="element" ns-uri="http://castor.org/library"/>
    </field>
  </class>
  <class name="org.castor.test.entity.Books" auto-complete="true">
    <map-to xml="Books" ns-uri="http://castor.org/library"/>
    <field name="book" type="org.castor.test.entity.Book" collection="array">
      <bind-xml name="Book" node="element" ns-uri="http://castor.org/library"/>
    </field>
  </class>
  <class name="org.castor.test.entity.Book" auto-complete="true">
    <map-to xml="Book" ns-uri="http://castor.org/library"/>
    <field name="title">
      <bind-xml name="Title" node="element" ns-uri="http://castor.org/library"/>
    </field>
    <field name="author" type="org.castor.test.entity.Author">
      <bind-xml name="Author" node="element" ns-uri="http://castor.org/library" reference="true"/>
    </field>
  </class>
  <class name="org.castor.test.entity.BookType" auto-complete="true">
    <map-to xml="BookType" ns-uri="http://castor.org/library"/>
    <field name="title">
      <bind-xml name="Title" node="element" ns-uri="http://castor.org/library"/>
    </field>
    <field name="author" type="org.castor.test.entity.Author">
      <bind-xml name="Author" node="element" ns-uri="http://castor.org/library"/>
    </field>
  </class>
</mapping>
