<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapping PUBLIC "-//EXOLAB/Castor Mapping DTD Version 1.0//EN"
   "http://castor.org/mapping.dtd">
<mapping>
  <class name="org.castor.test.entity.Emails" auto-complete="true">
    <map-to xml="emails" ns-uri="http://castor.org/email"/>
    <field name="email" type="org.castor.test.entity.Email" collection="array">
      <bind-xml name="email" node="element" ns-uri="http://castor.org/email"/>
    </field>
  </class>
  <class name="org.castor.test.entity.Email" auto-complete="true">
    <map-to xml="email" ns-uri="http://castor.org/email"/>
    <field name="from">
      <bind-xml name="from" node="element" ns-uri="http://castor.org/email"/>
    </field>
    <field name="to">
      <bind-xml name="to" node="element" ns-uri="http://castor.org/email"/>
    </field>
  </class>
</mapping>
